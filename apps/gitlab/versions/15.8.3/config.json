{
  "formFields": [
    {
      "type": "text",
      "labelZh": "访问地址",
      "labelEn": "Address",
      "required": true,
      "default": "",
      "envKey": "GITLAB_HOSTNAME",
      "placeholderZh": "请填写域名或者 IP 地址",
      "placeholderEn": "Please fill in the domain name or IP address"
    },
    {
      "type": "number",
      "labelZh": "HTTP 端口",
      "labelEn": "HTTP Port",
      "required": true,
      "default": 8090,
      "envKey": "PANEL_APP_PORT_HTTP"
    },
    {
      "type": "number",
      "labelZh": "HTTPS 端口",
      "labelEn": "HTTPS Port",
      "required": true,
      "default": 8443,
      "envKey": "PANEL_APP_PORT_HTTPS"
    },
    {
      "type": "number",
      "labelZh": "SSH 端口",
      "labelEn": "SSH Port",
      "required": true,
      "default": 2229,
      "envKey": "JENKINS_SSH_PORT"
    }
  ]
}