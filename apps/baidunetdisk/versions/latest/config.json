
{  
  "formFields": [ 
    {  
      "type": "number",
      "labelZh": "网页端口",  
      "labelEn": "WebUI Port",  
      "required": true,  
      "default": 40030,  
      "rule": "paramPort",  
      "envKey": "PANEL_APP_PORT_HTTP",  
      "edit": true  
    },
    {  
      "type": "password",
      "labelZh": "访问密码",  
      "labelEn": "Access Password",  
      "required": true,  
      "default": "VNCPassword",  
      "random": true,  
      "rule": "paramComplexity",  
      "envKey": "VNC_PASSWORD",  
      "edit": true  
    },
    {  
      "type": "text",
      "labelZh": "配置文件所在路径",  
      "labelEn": "Config file path",  
      "required": true,  
      "default": "./data/config",  
      "envKey": "CONFIG_PATH",  
      "edit": true  
    },
    {  
      "type": "text",
      "labelZh": "下载文件夹路径",  
      "labelEn": "Download folder path",  
      "required": true,  
      "default": "./data/downloads",  
      "envKey": "DOWNLOAD_PATH",  
      "edit": true  
    }
  ]  
}
