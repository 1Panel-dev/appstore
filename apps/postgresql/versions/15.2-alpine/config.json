{
  "formFields": [
    {
      "type": "text",
      "labelZh": "用户名",
      "labelEn": "Username",
      "required": true,
      "default": "user",
      "random": true,
      "rule": "paramCommon",
      "envKey": "PANEL_DB_ROOT_USER"
    },
    {
      "type": "password",
      "labelZh": "用户密码",
      "labelEn": "Password",
      "required": true,
      "default": "password",
      "random": true,
      "rule": "paramComplexity",
      "envKey": "PANEL_DB_ROOT_PASSWORD"
    },
    {
      "type": "number",
      "labelZh": "端口",
      "labelEn": "Port",
      "required": true,
      "default": 5432,
      "rule": "paramPort",
      "envKey": "PANEL_APP_PORT_HTTP"
    }
  ]
}