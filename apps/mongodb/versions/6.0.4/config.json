{
  "formFields": [
    {
      "type": "text",
      "labelZh": "Root 用户名",
      "labelEn": "Root Username",
      "required": true,
      "default": "mongo",
      "random": true,
      "rule": "paramCommon",
      "envKey": "PANEL_DB_ROOT_USER"
    },
    {
      "type": "password",
      "labelZh": "Root 用户密码",
      "labelEn": "Root Password",
      "required": true,
      "default": "mongo",
      "random": true,
      "rule": "paramComplexity",
      "envKey": "PANEL_DB_ROOT_PASSWORD"
    },
    {
      "type": "number",
      "labelZh": "端口",
      "labelEn": "Port",
      "required": true,
      "default": 27017,
      "rule": "paramPort",
      "envKey": "PANEL_APP_PORT_HTTP"
    }
  ]
}