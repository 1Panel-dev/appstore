{
  "formFields": [
    {
      "type": "text",
      "labelZh": "用户",
      "labelEn": "User",
      "required": true,
      "default": "minio",
      "random": true,
      "rule": "paramCommon",
      "envKey": "MINIO_ROOT_USER"
    },
    {
      "type": "password",
      "labelZh": "密码",
      "labelEn": "Password",
      "required": true,
      "default": "minio",
      "random": true,
      "rule": "paramComplexity",
      "envKey": "MINIO_ROOT_PASSWORD"
    },
    {
      "type": "number",
      "labelZh": "端口",
      "labelEn": "Port",
      "required": true,
      "default": 9000,
      "rule": "paramPort",
      "envKey": "PANEL_APP_PORT_HTTP"
    },
    {
      "type": "number",
      "labelZh": "API 端口",
      "labelEn": "Port",
      "required": true,
      "default": 9001,
      "rule": "paramPort",
      "envKey": "PORT_API"
    },
    {
      "type": "text",
      "labelZh": "分享文件地址",
      "labelEn": "SHARE ADRESS",
      "required": true,
      "default": "http://localhost",
      "rule": "paramExtUrl",
      "envKey": "MINIO_SERVER_URL",
      "edit": true
    }
  ]
}